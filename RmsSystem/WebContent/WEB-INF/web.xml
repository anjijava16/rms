<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <context-param>
    <param-name>log4j-config-location</param-name>
    <param-value>WEB-INF/log4j.xml</param-value>
  </context-param>
  <display-name>IRPC</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>StartUpServlet</display-name>
    <servlet-name>StartUpServlet</servlet-name>
    <servlet-class>com.iwinner.rms.web.servlet.AuditStartUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StartUpServlet</servlet-name>
    <url-pattern>/startUp.do</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.iwinner.rms.web.servlet.LoggingInitiazer</listener-class>
  </listener>
  <servlet>
    <description>LoginServlet Information</description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.iwinner.rms.web.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SchedulerServiceServlet</display-name>
    <servlet-name>SchedulerServiceServlet</servlet-name>
    <servlet-class>com.iwinner.rms.web.servlet.SchedulerServiceServlet</servlet-class>
    <init-param>
      <param-name>interval</param-name>
      <param-value>5</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>SchedulerServiceServlet</servlet-name>
    <url-pattern>/schedulerService</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ItemServlet</display-name>
    <servlet-name>ItemServlet</servlet-name>
    <servlet-class>com.iwinner.rms.web.servlet.ItemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ItemServlet</servlet-name>
    <url-pattern>/addItem.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ItemServlet</servlet-name>
    <url-pattern>/viewAllItems.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ItemServlet</servlet-name>
    <url-pattern>/editUser.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ItemServlet</servlet-name>
    <url-pattern>/deleteItem.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ItemServlet</servlet-name>
    <url-pattern>/updateItem.action</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>HomeInformationServvlet</display-name>
    <servlet-name>HomeInformationServvlet</servlet-name>
    <servlet-class>com.iwinner.rms.web.servlet.HomeInformationServvlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeInformationServvlet</servlet-name>
    <url-pattern>/homeServlet.action</url-pattern>
  </servlet-mapping>
</web-app>